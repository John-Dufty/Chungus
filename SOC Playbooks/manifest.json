{
  "manifest_version": 3,
  "name": "Chungus",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "storage",
    "<all_urls>"
  ],
  "action": {
    "default_popup": "options.html",
    "default_icon": {
      "128": "icons/icon128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["style.css"]
    }
  ]
}
